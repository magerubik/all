<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Magerubik_All::css/config.css"/>
    </head>
    <body>
        <referenceContainer name="messages.wrapper">
            <block class="Magerubik\All\Block\Adminhtml\Messages" name="mrall.messages" template="Magerubik_All::messages.phtml" after="page.messages"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Magerubik\All\Block\Adminhtml\Notification" name="mrall.notification" template="Magerubik_All::notification.phtml"/>
        </referenceContainer>
    </body>
</page>
